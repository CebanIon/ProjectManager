@{
    Layout = null;
}
@model ProjectManager.Application.Projects.Queries.ModifyProject.ModifyProjectQuery;
@if (ViewBag.SelectedProject != null)
{
    <div class="container">
        <div class="card shadow">
            <div class="card-header">
                Tasks
            </div>
            <div class="card-body">
                <a onClick="goToCreateTask(@ViewBag.ProjectId)" class="btn btn-primary float-end" tabindex="-1" role="button" aria-disabled="true">Create Task</a>
                <br>
                <br>
                <table id="example" class="mt-2 display" style="width:100%">
                </table>
            </div>
        </div>
    </div>
}